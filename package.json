{
  "name": "countup.js-angular2",
  "version": "1.1.0",
  "description": "Animates a numerical value by counting to it.",
  "main": "dist/bundles/countup.js-angular2.umd.js",
  "scripts": {
    "test": "ng test",
    "build": "ng-packagr -p package.json && rm dist/package.json && rm dist/README.md"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/inorganik/countUp.js-angular2.git"
  },
  "keywords": [
    "UI",
    "animation",
    "numbers"
  ],
  "author": "@inorganik",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/inorganik/countUp.js-angular2/issues"
  },
  "homepage": "http://inorganik.github.io/countUp.js",
  "dependencies": {
    "@angular/compiler": "^5.0.0",
    "@angular/compiler-cli": "^5.0.0",
    "@angular/common": "^4.2.4",
    "@angular/core": "^4.2.4",
    "countup.js": "^1.9.3"
  },
  "devDependencies": {
    "ng-packagr": "2.0.0-rc.9",
    "typescript": "~2.4"
  },
  "ngPackage": {
    "lib": {
      "entryFile": "index.ts",
      "umdModuleIds": {
        "countup.js": "CountUp"
      },
      "externals": {
        "countup.js": "CountUp"
      }
    }
  }
}
